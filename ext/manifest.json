{
    "manifest_version":2,
    "name": "Re: Mind",
    "version":"1.0",
    "description":"Store all your incomplete, favourite and save-for-later articles, blogs, news, podcasts and many more at one place",
    "icons":{
        "128":"./logo.png",
        "48":"./logo.png",
        "16":"./logo.png"
    },
    "browser_action":{
        "default_icon":"./logo.png",
        "default_popup":"popup.html"
    },
    "permissions":[
        "storage",
        "tabs",
        "activeTab"
    ]
}