{
    "manifest_version":2,
    "name":"Re: Mind",
    "version":"1.0",
    "description":"An extension that stores all the articles and blogs at one place.",
    "icons":{
        "128":"./assets/logo.png",
        "48":"./assets/logo.png",
        "16":"./assets/logo.png"
    },
    "browser_action":{
        "default_icon":"./assets/logo.png",
        "default_popup":"addblog.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'"

}